@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>My Tickets</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
          rel="stylesheet"
          integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM"
          crossorigin="anonymous" />
    <!-- font awesome -->
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
          integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw=="
          crossorigin="anonymous"
          referrerpolicy="no-referrer" />
    <style>
        /*  */
        .navbar {
            background-image: linear-gradient( 256deg, rgb(0, 45, 161), rgb(2, 108, 223) 55%, rgb(0, 45, 161) );
            width: 100%;
        }

        .navbar-scrolled {
            background-color: red;
        }

        .navbar a {
            color: white;
        }

            .navbar a:hover {
                color: rgba(255, 255, 255, 0.8);
            }

            .navbar a:focus {
                color: rgba(255, 255, 255, 0.8);
            }

        .inputform input::placeholder {
            font-size: 14px;
            color: black;
        }

        .outerlink a {
            font-size: 10px;
            text-decoration: none;
        }

        .inputform div,
        .inputform input {
            border: none;
            background-color: white;
            opacity: 0.5;
            padding: 5px;
        }

        .inputform input {
            border-radius: 0 200px 200px 0;
            outline: none;
        }

        .inputform div {
            border-radius: 200px 0 0 200px;
        }

        .inputform input:focus {
            box-shadow: none;
        }

        .navbar-collapse ul li {
            transition: 0.7s;
        }

            .navbar-collapse ul li:hover {
                background-color: rgba(0, 0, 0, 0.1);
            }

        .navbar-nav li:hover a {
            color: white;
            /* 保留連結內文字的原始顏色 */
        }

        .personIcon {
            fill: #fff;
        }
        /*  */
        a {
            text-decoration: none;
            color: black;
        }

        ul {
            list-style: none;
        }

        .gridTable {
            position: relative;
            z-index: 0;
            display: grid;
            grid-template-rows: auto;
            grid-template-columns: 100%;
            background-color: rgb(246, 246, 246);
        }

        .ticketHeaderAllBlock {
            z-index: -1;
            grid-area: 1/1/2/-1;
        }

        .ticketHeaderBackground {
            background-color: rgb(2, 77, 223);
            background-image: linear-gradient( 89deg, rgb(1, 80, 167), rgb(2, 77, 223), rgb(1, 80, 167) );
            position: relative;
            width: 100%;
            height: 100%;
        }
        /* 左側收合欄上方LOGO區塊 */
        .ticketLogoBlock {
            display: none;
        }

        .ticketMonsterIcon {
            width: 40px;
            height: 40px;
            margin-left: -8px;
            color: white;
            fill: white;
        }

        .welcome p {
            padding-top: 15px;
            margin-bottom: 0;
        }
        /* 側邊收合欄區域 */
        .ticketSide {
            display: none;
            background-color: #fff;
            padding-bottom: 88px;
            box-shadow: rgba(0, 0, 0, 0.15) 0px 0px 10px 3px;
        }

        .accordion:nth-child(-n + 4):after {
            display: block;
            border-top: 1px solid rgb(191, 191, 191);
            background-color: #fff;
            margin-top: 10px;
            margin-bottom: 10px;
            content: "";
        }

        .myTicketsIcon {
            fill: gray;
        }

        .accordion-body {
            padding-left: 60px;
        }

        .accordion-item .accordion-button {
            font-weight: 900;
            padding-bottom: 10px;
            color: black;
        }

        .accordion-button:not(.collapsed) {
            background-color: rgb(255, 255, 255);
            box-shadow: none;
        }

        .accordion-button:hover {
            background-color: rgb(246, 246, 246);
        }

        .accordion-button:nth-last-child(1):after {
            display: none;
        }

        .accordion .btnRed {
            color: red;
        }

        .accordion-button::after,
        .accordion-button:not(.collapsed)::after {
            background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 16 16' fill='%230c63e4'%3e%3cpath fill-rule='evenodd' d='M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708z'/%3e%3c/svg%3e");
        }
        /* My tickets標題區 */
        .ticketHeader {
            z-index: -1;
            grid-area: 1/ 1 /2/-1;
            padding-top: 24px;
            font-size: 14px;
            margin-bottom: 40px;
        }

        .breadcrumb-item.active a,
        .breadcrumb-item.active,
        .breadcrumb-item + .breadcrumb-item::before {
            color: white;
        }

        .nav-item a {
            color: #fff;
        }

        .nav.nav-tabs {
            border-bottom: 1px solid rgba(255, 255, 255, 0.4);
        }

        .nav-tabs .nav-item .nav-link:focus {
            color: #fff;
        }

        .nav.nav-tabs .nav-link:hover {
            color: #fff;
            background-color: rgba(7, 169, 244, 0.4);
        }
        /* 門票資訊 */
        .ticketCards {
            padding-bottom: 88px;
        }

        .ticketCardsWrap {
            padding-right: 30px;
            margin-left: 0px;
        }

        .ticketCards svg {
            top: 50%;
            right: 16px;
            transform: rotate(-90deg) translateX(50%);
            fill: white;
        }

        .ticketCardImg {
            position: absolute;
            z-index: 0;
            background-image: url(./Will-Image/dino-Mytickets.jpg);
            width: calc(100% + 100px);
            height: calc(100% + 100px);
            background-position: center center;
            background-size: cover;
            transform: translate(-60px, -60px);
            filter: blur(15px);
        }

        .orderID {
            position: absolute;
            width: 100%;
            margin: 8px 0px 0px;
            padding-right: 16px;
            padding-left: 152px;
            text-align: right;
            font-size: 14px;
            opacity: 0.75;
            color: white;
        }

        .ticketCardsContext {
            color: white;
        }

            .ticketCardsContext img {
                position: relative;
                z-index: 0;
                display: block;
                height: auto;
                border-radius: 2px;
                width: 120px;
            }

        .ticketCards ul {
            margin: -16px 0px 0px;
        }

        figure {
            position: relative;
            overflow: hidden;
            box-shadow: rgba(0, 0, 0, 0.3) -4px 4px 8px 0px;
        }

        .eventTime::before {
            content: "- ";
        }

        .ticketCardsContextInfo {
            padding-top: 10px;
        }

        @@media (min-width: 1050px) {
            .gridTable {
                grid-template-rows: auto auto 1fr;
                grid-template-columns: minmax(60px, auto) 320px 56px minmax(0px, 50rem) minmax(60px, auto);
                margin-left: auto;
                margin-right: auto;
            }

            .ticketLogoBlock {
                display: flex;
                flex-direction: column;
                grid-area: 1/2/2/3;
                justify-content: flex-end;
                margin-top: 24px;
                padding: 32px;
                background-color: rgba(255, 255, 255, 0.1);
            }

            .ticketSide {
                display: block;
                grid-area: 2/2/-1/3;
            }

            .ticketHeader {
                grid-column: 4/5;
            }

            .ticketCards {
                grid-area: 3/4/-1/5;
            }

            .ticketCardsWrap {
                padding-right: 0px;
                margin-left: -30px;
            }
        }

        @@media (min-width: 768px) {
            .ticketCardsContext {
                display: flex;
            }

            .ticketCardsContextInfo {
                padding-top: 0px;
                padding-left: 20px;
            }
        }
        /* footer */
        .background {
            width: 100%;
            background-color: rgb(31, 38, 45);
            background-size: cover;
        }

            .background * {
                color: white;
            }

        .list-unstyled {
            display: none;
        }

        /* ----------------footer css---------------- */

        @@media (min-width: 992px) {
            .ticket img {
                vertical-align: top;
                transition: 1s;
                transform: scale(1);
            }

            .ticket:hover img {
                transform: scale(1.2);
            }

            .ticket {
                position: relative;
                border: none;
            }

            .list-unstyled {
                display: block;
            }

                .list-unstyled :first-child {
                    margin-top: 50px;
                    font-size: 16px;
                    font-weight: bold;
                }

                .list-unstyled li {
                    margin-top: 10px;
                    font-size: 14px;
                }

            .footer-span {
                border-top: 1px solid white;
            }
        }
        /* footer */
        @@media screen and (min-width: 768px) {
            .accountNameYear {
                text-align: center;
            }

            .EditProfileBtn {
                display: flex;
            }

            .mainRight {
                padding-left: 30px;
            }
        }

        @@media screen and (max-width: 767px) {
            .hiddenblockInCol {
                display: none;
            }

            .accounttitle {
                font-size: 24px;
            }

            .accountNameGp {
                display: flex;
            }

            .accountName {
                font-size: 16px;
            }
        }

        @@media screen and (max-width: 1023px) {
            .hiddenblockInMd {
                display: none;
            }

            .hiddenfooterblock {
                display: none;
            }

            .footerblockInLg {
                font-size: 10px;
            }
        }

        @@media screen and (min-width: 992px) {
            .ticketImg {
                position: relative;
                width: 60%;
                height: auto;
            }
        }
        /* footerEnd */
    </style>
</head>
<body>
    <!-- 導覽列 -->
    <nav class="navbar navbar-expand-lg sticky-top bg-primary">
        <div class="container-fluid mx-2">
            <a class="navbar-brand" href="@Url.Action("HomePageView", "HomePage")">TicketMonster</a>
